{
	"$schema": "vscode://schemas/color-theme",
	"name": "blackodin",
	"type": "dark",
	"colors": {
		"activityBar.background": "#101010",
		"editor.background": "#000000",
		"debugToolBar.background": "#101010",
		"dropdown.listBackground": "#101010",
		"editorGroupHeader.tabsBackground": "#101010",
		"editorWidget.background": "#101010",
		"menu.background": "#101010",
		"notificationCenterHeader.background": "#101010",
		"notifications.background": "#101010",
		"peekViewEditor.background": "#101010",
		"sideBar.background": "#101010",
		"sideBarSectionHeader.background": "#101010",
		"statusBar.background": "#101010",
		"statusBar.debuggingBackground": "#004080",
		"statusBar.noFolderBackground": "#101010",
		"tab.activeBackground": "#080808",
		"tab.inactiveBackground": "#181818",
		"titleBar.activeBackground": "#101010",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment", // //
			"scope": [
				"comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "String", // "abc"
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#ff9977"
			}
		},
		{
			"name": "Constant", // 20
			"scope": "constant",
			"settings": {
				"foreground": "#ff9977"
			}
		},
		{
			"name": "Function name", // my_proc
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#ffff88"
			}
		},
		{
			"name": "Embedded String Begin/End", // my_func() - para
			"scope": [
				"string.embedded.begin",
				"string.embedded.end",
				"punctuation.definition.template-expression",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#ffff88",
			}
		},
		{
			"name": "Default Embedded Color", // period. between fields
			"scope": [
				"punctuation.separator",
				"punctuation.accessor",
				"meta.brace"
			],
			"settings": {
				"foreground": "#aaaaaa",
			}
		},
		{
			"name": "obj block definition.block", // obj definition { curly
			"scope": [
				"punctuation.definition.block"
			],
			"settings": {
				"foreground": "#aaaaaa",
			}
		},
		{
			"name": "typeparameters", // type definition ( para
			"scope": [
				"punctuation.definition.typeparameters"
			],
			"settings": {
				"foreground": "#aaaaaa",
			}
		},
		{
			"name": "proc and return parameters", // proc call ( para
			"scope": [
				"punctuation.definition.parameters"
			],
			"settings": {
				"foreground": "#aaaaaa",
			}
		},
		{
			"name": "Keyword Operator", // ::, ^, &, :=, =, :
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#aaaaaa",
				"fontStyle": ""
			}
		},
		{
			"name": "keyword.control",
			"scope": [
				"keyword.control", // package if|else|or_else|when|where|for|in|not_in|defer|switch|return|or_return break|or_break|continue|or_continue fallthrough|case|dynamic do|force_inline|no_inline asm
				"keyword.control.import", // import, foreign
				"keyword.control.attribute", // @attribute
				"keyword.control.case", // case
				"keyword.other", // proc
				"keyword.function", // cast,transmute
			],
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
			"name": "Types", // struct, union, enum, distinct, ->
			"scope": [
				"storage.type",
				"storage.type.function.arrow",
				"punctuation.definition.bracket.square"
			],
			"settings": {
				"foreground": "#6688ff",
				"fontStyle": ""
			}
		},
		{
			"name": "Variable Name", // gc.valid_moves, CONST, Type
			"scope": [
				"variable.name"
			],
			"settings": {
				"foreground": "#66ddff",
				"fontStyle": ""
			}
		},
		{
			"name": "Variable Object", // gc.valid_moves, CONST, Type
			"scope": [
				"variable.other.object",
				"variable.other.constant",
				"variable.other.global",
				"variable.other.readwrite.class",
				"variable.other.readwrite.instance",
				"variable.other.readwrite.batchfile",
				"variable.readwrite",
				"variable.readwrite.other.block"
			],
			"settings": {
				"foreground": "#22aaee",
				"fontStyle": ""
			}
		},
		{
			"name": "Package Name / Namespace", // My_Class
			"scope": [
				"entity.name.type.module",
				"entity.name.namespace"
			],
			"settings": {
				"foreground": "#44ffbb",
				"fontStyle": "underline",
			}
		},
		{
			"name": "Class", // My_Class
			"scope": [
				"entity.name.class",
				"entity.name.module",
				"entity.name.type",
				"storage.identifier",
				"support.class"
			],
			"settings": {
				"foreground": "#44ffbb",
				"fontStyle": "underline",
			}
		},
		{
			"name": "Library class/type", // int, bool
			"scope": [
				"support.type",
				"support.variable"
			],
			"settings": {
				"foreground": "#00dd99",
				"fontStyle": ""
			}
		}
	]
}